{
  "name": "uportlandia",
  "version": "1.0.0",
  "private": true,
  "homepage": "https://uportlandia.uport.me",
  "repository": "https://github.com/uport-project/cleverland",
  "devDependencies": {
    "@sentry/webpack-plugin": "^1.6.2",
    "chai": "^4.2.0",
    "cloudfront-invalidate-cache": "^1.0.1",
    "react-app-rewired": "^2.1.3",
    "react-scripts": "^2.1.8",
    "s3-deploy": "^1.2.1",
    "serverless-offline": "^4.9.4",
    "serverless-plugin-include-dependencies": "^3.2.1"
  },
  "dependencies": {
    "@sentry/browser": "5.1.1",
    "body-parser": "^1.18.3",
    "connected-react-router": "^6.4.0",
    "dayjs": "^1.8.0",
    "i18next": "^15.0.9",
    "i18next-browser-languagedetector": "^3.0.1",
    "localforage": "^1.7.3",
    "mobile-detect": "^1.4.3",
    "qr-image": "^3.2.0",
    "qs": "^6.7.0",
    "react": "^16.8.6",
    "react-dom": "^16.8.6",
    "react-i18next": "^10.7.0",
    "react-redux": "^7.0.3",
    "react-router": "4.3.1",
    "react-router-dom": "4.3.1",
    "react-tooltip": "^3.9.2",
    "redux": "^4.0.1",
    "redux-saga": "0.16.0",
    "reselect": "3.0.1",
    "serverless": "^1.41.0",
    "serverless-express": "^2.0.11",
    "serverless-scriptable-plugin": "^1.0.0",
    "shortid": "2.2.12",
    "styled-components": "4.1.2",
    "uport-transports": "^0.2.5"
  },
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "predeploy": "npm run build",
    "test": "react-scripts test --env=jsdom",
    "eject": "react-scripts eject",
    "create-did": "node node_scripts/create_did.js",
    "s3-upload": "node_modules/.bin/s3-deploy './build/**' './build/.well-known/**' --cwd './build/' --region us-west-2 --bucket",
    "deploy-secrets": "./deploy-hooks/deploy-secrets.sh",
    "deploy:stage": "REACT_APP_TARGET_ENV=stage npm run build; npm run create-did stage; npm run s3-upload cleverland-stage && yarn invalidate uportlandia.uport.space",
    "deploy:production": "REACT_APP_TARGET_ENV=production npm run build; npm run create-did production; npm run s3-upload cleverland-prod && yarn invalidate uportlandia.uport.me",
    "invalidate": "node_modules/.bin/cloudfront-invalidate-cache --cname",
    "deploy:sls": "sls deploy -v -s prod",
    "local:api": "sls offline -s prod"
  },
  "browserslist": [
    ">0.2%",
    "not dead",
    "not ie <= 11",
    "not op_mini all"
  ]
}
